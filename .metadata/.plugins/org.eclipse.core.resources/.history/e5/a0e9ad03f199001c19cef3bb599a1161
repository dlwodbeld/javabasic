package pm;

public class Exam6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		char value1 = 'A';
		char value2 = '가';
		char value3 = '3';
		
		System.out.println(value1);
		System.out.println(value2);
		System.out.println(value3);
		System.out.println();
		
		// 숫자로 문자 저장
		char value4 = 65;
		System.out.println(value4);
		System.out.println();
		
		// 유니코드로 문자 저장
		
		char value5 = '\u0065';
		System.out.println(value5);
		System.out.println();
		

	}

}
